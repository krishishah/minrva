<?xml version="1.0" encoding="UTF-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="minrva.Profile">
	<ContentPage.Padding>
 		<OnPlatform x:TypeArguments="Thickness" iOS="10, 20, 10, 10" WinPhone="20,20,20,20" />
 	</ContentPage.Padding>
	<ContentPage.Content>
		<Grid>			
			<BoxView BackgroundColor="White" Grid.Row="0" Grid.Column="0" Grid.RowSpan="3" Grid.ColumnSpan="1" />
			<BoxView BackgroundColor="Green" Grid.Row="0" Grid.Column="1" Grid.RowSpan="1" Grid.ColumnSpan="2"/>
			<BoxView BackgroundColor="Green" Grid.Row="1" Grid.Column="1" Grid.RowSpan="1" Grid.ColumnSpan="2"/>
			<BoxView BackgroundColor="Green" Grid.Row="2" Grid.Column="1" Grid.RowSpan="1" Grid.ColumnSpan="2"/>
			<BoxView BackgroundColor="Green" Grid.Row="3" Grid.Column="0" Grid.RowSpan="1" Grid.ColumnSpan="3"/>
			<BoxView BackgroundColor="Green" Grid.Row="4" Grid.Column="0" Grid.RowSpan="8" Grid.ColumnSpan="3"/>
			<Button x:Name="signOutButton" Text="Sign-out"
    			  Clicked="signOutButton_Clicked"/>
		</Grid>
	</ContentPage.Content>
</ContentPage>
